{
  "name": "Keyboard Control for Theorieexamen.nl",
  "description": "Go through your tests on Theorieexamen.nl faster using a keyboard instead of a mouse!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.theorieexamen.nl/*"],
      "js": ["main.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/img/logo_16.png",
      "32": "/img/logo_32.png",
      "48": "/img/logo_48.png",
      "128": "/img/logo_128.png"
    }
  },
  "icons": {
    "16": "/img/logo_16.png",
    "32": "/img/logo_32.png",
    "48": "/img/logo_48.png",
    "128": "/img/logo_128.png"
  }
}